# before using futaba_match
$$ cd ../tsubomi
$$ gmake
$$ sudo gmake install
$$ cd sample
$$ gmake
$$ sudo gmake install


# data format
see var/color.txt


# make index
$$ futaba_make_tfidf var/color.txt > var/color.tfidf
$$ tsubomi_mkary -l var/color.tfidf


# use futaba_match
$$ futaba_match var/color.tfidf < var/color.txt

